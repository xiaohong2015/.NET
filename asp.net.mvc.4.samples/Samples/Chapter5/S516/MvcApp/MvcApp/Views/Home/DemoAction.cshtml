@model IDictionary<string, object>
<html>
<head>
    <title>绑定的参数</title>
    <link rel="stylesheet"  href="~/Style.css" />
</head>
    <body>
        <table rules="all">
            <tr><th>Name</th><th>Value</th></tr>
            @foreach (var item in Model)
            { 
                <tr><td>@item.Key</td><td>@item.Value</td></tr>
            }
        </table>
    </body>
</html>
